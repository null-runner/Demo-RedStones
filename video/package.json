    "remotion": "4.0.409",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/web": "^0.0.138",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "@remotion/tailwind": "4.0.409",
    "@remotion/cli": "4.0.409",
    "@remotion/gif": "4.0.409"
  }
}
