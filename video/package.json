{
  "name": "redstones-crm-video",
  "version": "1.0.0",
  "private": true,
  "description": "Hero GIF for RedStones CRM README",
  "scripts": {
    "start": "remotion preview src/index.tsx",
    "build:gif": "remotion render src/index.tsx CrmHero --image-format=png --output=out/hero.gif --codec=gif",
    "build:mp4": "remotion render src/index.tsx CrmHero --output=out/hero.mp4"
  },
